@using WebApp.Presentacion.WebMvc5.Resources

@model WebApp.Presentacion.WebMvc5.ViewModels.Nt_CantCrudViewModel

@{ 
	ViewBag.Title = "Remove Nt_Cant"; 
	}

<h2>@ViewBag.Title</h2>

<h3>@Html.Label(PresentationResources.DeleteSure)</h3>
@using (Html.BeginForm()) {
	@Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

	<fieldset>
		<legend>Nt_Cant</legend>

			@Html.HiddenFor(model => model.Nt_Cant.Id)

		<div class="newline">
			<div class="label">
				@Html.DisplayNameFor(model => model.Nt_Cant.IdAlim):
			</div>
			<div class="field">
				@Html.DisplayFor(model => model.Nt_Cant.IdAlim)
			</div>
			<div class="label">
				@Html.DisplayNameFor(model => model.Nt_Cant.IdNtFte):
			</div>
			<div class="field">
				@Html.DisplayFor(model => model.Nt_Cant.IdNtFte)
			</div>
		</div>
		<div class="newline">
			<div class="label">
				@Html.DisplayNameFor(model => model.Nt_Cant.IdNut):
			</div>
			<div class="field">
				@Html.DisplayFor(model => model.Nt_Cant.IdNut)
			</div>
			<div class="label">
				@Html.DisplayNameFor(model => model.Nt_Cant.Valor):
			</div>
			<div class="field">
				@Html.DisplayFor(model => model.Nt_Cant.Valor)
			</div>
		</div>
